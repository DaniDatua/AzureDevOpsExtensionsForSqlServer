{
    "manifestVersion": 1,
    "id": "DeployTabularModel",
    "version": "1.0.7",
    "name": "Deployment tools for SSAS Tabular Cube Models",
    "publisher": "DrJohnExtensions",
    "description": "Deploy, update and drop a tabular cube model on a Microsoft SQL Server Analysis Services (SSAS) server",
    "public": true,
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "deploy",
        "release",
        "ssas",
        "tabular",
        "cube"
    ],
    "icons": {
        "default": "images/microsoft-ssas.png"
    },
    "content": {
        "details": {
            "path": "readme.md"
        },
        "license": {
            "path": "license.md"
        }
    },
    "screenshots": [
        {
            "path": "images/microsoft-ssas-250.png"
        },
        {
            "path": "images/update-ssas-250.png"
        },
        {
            "path": "images/process_ssas_cube250.png"
        },
        {
            "path": "images/drop_ssas_cube250.png"
        }
    ],
    "links": {
        "getstarted": {
            "uri": "https://github.com/DrJohnT/AzureDevOpsExtensionsForSqlServer/wiki/Deploy-Tabular-Cube"
        },
        "issues": {
            "uri": "https://github.com/DrJohnT/AzureDevOpsExtensionsForSqlServer/issues"
        },
        "repository": {
            "uri": "https://github.com/DrJohnT/AzureDevOpsExtensionsForSqlServer"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/DrJohnT/AzureDevOpsExtensionsForSqlServer"
    },
    "branding": {
        "color": "#f6f7fb",
        "theme": "light"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "files": [
        {
            "path": "DeployTabularModelTask"
        },
        {
            "path": "UpdateTabularCubeDataSourceTask"
        },
        {
            "path": "ProcessTabularModelTask"
        },
        {
            "path": "DropCubeTask"
	    },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "DeployTabularModel",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "DeployTabularModelTask"
            }
        },
        {
            "id": "UpdateTabularCubeDS",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "UpdateTabularCubeDataSourceTask"
            }
        },
        {
            "id": "ProcessTabularModel",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "ProcessTabularModelTask"
            }
        },
        {
            "id": "DropCube",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "DropCubeTask"
            }
        }
    ]
}