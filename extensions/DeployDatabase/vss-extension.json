{
    "manifestVersion": 1,
    "id": "DeployDatabase",
    "name": "Deploy Database DACPAC using a DAC Publish Profile",
    "version": "1.0.2",
    "publisher": "DrJohnExtensions",
    "description": "Deploy a database DACPAC to a Microsoft SQL Server instance",
    "public": true,
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "deploy",
        "build",
        "sqldatabase",
        "sqlpackage"
    ],
    "icons": {
        "default": "images/deploy.png"
    },
    "content": {
        "details": {
            "path": "readme.md"
        }
    },
    "license": {
        "path": "license.md"
    },
    "screenshots": [
        {
            "path": "images/TaskDetail.png"
        }
    ],
    "links": {
        "getstarted": {
            "uri": "https://github.com/DrJohnT/AzureDevOpsExtensionsForSqlServer/wiki/Publish-DacPac"
        },
        "issues": {
            "uri": "https://github.com/DrJohnT/AzureDevOpsExtensionsForSqlServer/issues"
        },
        "repository": {
            "uri": "https://github.com/DrJohnT/AzureDevOpsExtensionsForSqlServer"
        }
    },
    "branding": {
        "color": "#f6f7fb",
        "theme": "light"
    },
    "scopes": [
        "vso.build_execute",
        "vso.serviceendpoint_manage"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "files": [
        {
            "path": "DeployDatabaseTask"
        }
    ],
    "contributions": [
        {
            "id": "DeployDatabase",
            "type": "ms.vss-distributed-task.task",
            "targets": [
            "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "DeployDatabaseTask"
            }
        }
    ]
}